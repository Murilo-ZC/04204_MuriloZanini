<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="455.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="carvalho.zanini.ControleInterface">
   <children>
      <VBox layoutX="14.0" layoutY="31.0" prefHeight="354.0" prefWidth="227.0" spacing="16.0">
         <children>
            <Label fx:id="lblSaida" text="Saida:" />
            <TextField fx:id="txtEntrada" promptText="Informe um valor" />
            <Button fx:id="btnBotao1" mnemonicParsing="false" onAction="#exibirNome" text="OK" />
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="24.0">
               <children>
                  <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="16.0">
                     <children>
                        <TextField fx:id="txtNumero1" />
                        <TextField fx:id="txtNumero2" />
                     </children>
                  </VBox>
                  <Button fx:id="btnSomar" mnemonicParsing="false" onAction="#somar" text="Button">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Button>
               </children>
               <padding>
                  <Insets top="16.0" />
               </padding>
            </HBox>
         </children>
         <padding>
            <Insets left="8.0" right="8.0" top="8.0" />
         </padding>
      </VBox>
      <VBox layoutX="341.0" layoutY="31.0" prefHeight="308.0" prefWidth="211.0" spacing="8.0">
         <children>
            <Label text="Cadastro:" />
            <TextField fx:id="txtNome" promptText="Nome:" />
            <TextField fx:id="txtSenha" promptText="Senha:" />
            <TextField fx:id="txtUrl" promptText="URL Foto:" />
            <Button mnemonicParsing="false" onAction="#previewPhoto" text="Preview Foto" />
            <ImageView fx:id="imgFoto" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
            <Button mnemonicParsing="false" onAction="#cadastrar" text="Cadastrar" />
         </children>
         <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
